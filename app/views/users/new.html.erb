<form action="/users" accept-charset="UTF-8" method="post" data-turbo="false">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <label for="username">User Name</label>
    <input type="text" name="user[username]" id="username"><br>
    <label for="email">Email</label>
    <input type="email" name="user[email]" id="email"><br>
    <label for="password">Password</label>
    <input type="password" id="password" name="user[password]"><br>
    <input type="submit" value="Submit">

</form>